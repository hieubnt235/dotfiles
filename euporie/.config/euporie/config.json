{
    "tab_size": 4,
    "color_scheme": "default",
    "language_servers": {
        "ruff": {
            "command": [
                "ruff",
                "server"
            ],
            "languages": [
                "python"
            ]
        }
    },
    "formatters": [
        {
            "command": [
                "ruff",
                "format",
                "-"
            ],
            "languages": [
                "python"
            ]
        }
    ],
    "multiplexer_passthrough": true,
    "enable_language_servers": true,
    "autocomplete": true,
    "autosuggest": true,
    "autoinspect": true,
    "autoformat": true,
    "line_numbers": true,
    "edit_mode": "vi",
    "external_editor": "nvim",
    "graphics": "kitty-unicode",
    "force_graphics": true,
    "show_file_icons": true,
    "show_side_bar": true,
    "show_filenames": true,
    "always_show_tab_bar": true,
    "tab_mode": "stack",
    "show_cell_borders": true
}
